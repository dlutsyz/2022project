    "import requests\n",
    "\n",
    "#MIT\n",
    "# 执行API调用并存储响应\n",
    "url = 'https://api.github.com/search/repositories?q=created:2021+license:mit'\n",
    "headers = {'Accept': 'application/vnd.github.v3+json'}\n",
    "r = requests.get(url, headers=headers)\n",
    "print(f\"Status code: {r.status_code}\")\n",
    "\n",
    "# 将API响应赋给一个变量\n",
    "response_dict = r.json()\n",
    "count2021MIT = response_dict['total_count']\n",
    "print(f\"Total repositories: {count2021MIT}\")\n",
    "\n",
